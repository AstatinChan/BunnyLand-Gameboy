Heal_Attack:
	LD A, $mem_bunny_health
	ADD $03
	DAA
	CP $20
	JR C, =.Set_health
	LD A, $20
	.Set_health:
	LD $mem_bunny_health, A

	PUSH BC
	LD A, $mem_bunny_x
	LD B, A
	LD A, $mem_bunny_y
	LD C, A
	LD A, $01
	CALL =Try_Launch_Animation
	POP BC

	LD A, $enum_dungeon_mode
	LD $mem_requested_mode, A
	LD $mem_current_mode, A

	.CLOSE_DIALOGUE

	LD A, $mem_map_loading_flags
	SET 3, A
	LD $mem_map_loading_flags, A
	RET
